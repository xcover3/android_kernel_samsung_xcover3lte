/dts-v1/;
#include "pxa1936-board-common.dtsi"

&pmx {
	mfp_pins_group_0: mfp_pins_group_0 {
		pinctrl-single,pins = <
			DF_IO9 AF0
			DF_IO10 AF0
			DF_IO11 AF0
			DF_IO12 AF0
			DF_IO13 AF0
			DF_IO15 AF0
		>;
		DS_MEDIUM;PULL_DOWN;EDGE_NONE;LPM_NONE;
	};

	mfp_pins_group_2: mfp_pins_group_2 {
		pinctrl-single,pins = <
			GPIO12 AF0
			GPIO13 AF0
			GPIO14 AF0
			GPIO15 AF0
		>;
		DS_MEDIUM;PULL_UP;EDGE_NONE;LPM_NONE;
	};

	disp_func: disp_func {
		pinctrl-single,pins = <
			DF_IO8	AF1
		>;
		MFP_LPM_PULL_DW;
	};

	touch_pins_aloe: touch_pins_aloe {
		pinctrl-single,pins = <
			GPIO76 AF0
			GPIO78 AF0
			GPIO98 AF0
		 >;
		 MFP_LPM_FLOAT;
	 };
};

&disp {
	pinctrl-0 = <&disp_func>;
};
